#!/bin/bash
git clone https://github.com/magwas/zenta-tools.git
export ZENTATOOLS=$(pwd)/zenta-tools
export PATH=$ZENTATOOLS/bin:$PATH
export DISPLAY=:99.0
/sbin/start-stop-daemon --start --quiet --pidfile /tmp/cucumber_xvfb_99.pid --make-pidfile --background --exec /usr/bin/Xvfb -- :99 -ac -screen 0 1024x768x24
make

